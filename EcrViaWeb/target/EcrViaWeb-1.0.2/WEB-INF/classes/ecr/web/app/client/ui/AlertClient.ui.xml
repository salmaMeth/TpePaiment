<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	  xmlns:my='urn:ecr.web.app.client.services'>
		<ui:style ui:field="style" type='ecr.web.app.client.services.MyStyle'>
	
	
    	.mainPanel {
                      border: 2px solid black;
           
        }
        
        .ERROR {
	  font-size: small;
      color: red;
      font-weight: bold;
        }
          .success {
	  font-size: small;
       color: green;
      font-weight: bold;

        }
        .toolcss { }
        .style {}
        .UNKNOWN {}
        .IDLE {}
        .BUSY {}
        .CARD_INSERT {}   
        .PIN_VERIFY {}     
        .AUTHORISING {}
	    .COMPLETION {}  
        .CANCELLED {} 

    </ui:style>
	<g:HTMLPanel styleName="{style.mainPanel}"> 
	<div class="row-fluid ">
<div class="span12">
<div class="content-widgets gray">
<div class="stepy-widget">
<div class="widget-head blue clearfix">
<h3 class="pull-left">Search By ID </h3>

<div id="stepy_tabby" class="pull-right">
<ul id="stepy-titles" class="stepy-titles">
<li id="stepy-title-0" class="current-step">
<div><g:Image   width="51px" height="47px" ui:field="image" ></g:Image></div>
<span> <center>Photos</center></span>
</li></ul></div></div></div>
<div class="widget-container">
		<g:VerticalPanel width="537px" height="87px" horizontalAlignment="ALIGN_LEFT">
				<g:HorizontalPanel width="535px" height="28px" verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_LEFT">
					<g:Cell verticalAlignment="ALIGN_MIDDLE">
						<g:TextBox height="38px" width="320px" ui:field="txt_search"/>
					</g:Cell>
					<g:Button width="162px" height="32px" text="Serach" ui:field="btn_search"/>
			</g:HorizontalPanel>
			<g:Button height="32px" width="121px" text="Cancel" ui:field="btn_canncel"/>
		</g:VerticalPanel>
				</div>
</div></div>
</div>
	</g:HTMLPanel>
	
</ui:UiBinder> 