	<ui:style>
  table
{
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-collapse: separate;
    border-color: #dddddd #dddddd #dddddd -moz-use-text-color;
    border-image: none;
    border-width: 1px 1px 1px 0;
    width: 100%;
}

td{
    line-height: 20px;
    text-align: left;
    vertical-align: top;
    color: black;
    border-collapse: collapse;
}


	</ui:style>

<g:HTMLPanel height="1500" width="1500" ui:field="container">
	<g:HorizontalPanel width="880px" height="283px">
		<g:VerticalPanel height="275px" horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE" width="826px">
			<g:HorizontalPanel height="42px" horizontalAlignment="ALIGN_LEFT"  verticalAlignment="ALIGN_BOTTOM" width="789px">
				<g:HorizontalPanel height="38px" horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_BOTTOM" width="400px">
					<g:Label height="22px" horizontalAlignment="ALIGN_CENTER" text="Find by Id" width="156px"/>
