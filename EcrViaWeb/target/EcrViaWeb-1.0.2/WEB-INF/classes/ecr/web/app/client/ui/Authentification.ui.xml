<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.span12 {
		border: 1px solid #006600;
		font-size: 14px;
		height: auto;
		margin-bottom: 15px;
		padding: 6px 6px 6px 30px;
		background-color: #FFFFFF;
		transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;
		color: #555555;
		display: inline-block;
		line-height: 20px;
		vertical-align: middle;
		height: 35px !important;
	}
	
	.textBox {
		width: 263px !important;
	}
	
	.button-black {
		border: 1px solid rgba(0, 0, 0, 0.1);
		padding: 6px 12px;
		background-color:  #282828;
		color: #FFFFFF;
		-moz-box-sizing: border-box;
		display: block;
		width: 100%;
		cursor: pointer;
		font-size: 12px;
		line-height: 20px;
		margin-bottom: 0;
		text-align: center;
		vertical-align: middle;
	}
	.button-black:HOVER {
		background-color: #7C798F;
	}
	.button-green{
		border: 1px solid rgba(0, 0, 0, 0.1);
		padding: 6px 12px;
		background-color: #009600;
		color: #FFFFFF;
		-moz-box-sizing: border-box;
		display: block;
		width: 100%;
		cursor: pointer;
		font-size: 12px;
		line-height: 20px;
		margin-bottom: 0;
		text-align: center;
		vertical-align: middle;
	}
	.button-green:HOVER {
		background-color: #014401;
	}
	</ui:style>
	<g:HTMLPanel ui:field="htmll">
		<div class="layout">

			<div class="navbar navbar-inverse top-nav">
				<div class="navbar-inner">
					<div class="container">
						<span class="home-link">
						<p>
						
						<g:Anchor ui:field="lienhome" href="javascript:;"><a class="icon-home"></a></g:Anchor>								
				      	</p>
							
						</span>
					
						<div class="btn-toolbar pull-right notification-nav">
							<div class="btn-group">
								<div class="dropdown">
									<a class="btn btn-notification">
									<g:Anchor ui:field="lienreply" href="javascript:;">
							      <i class="icon-reply"></i>
									</g:Anchor>								
									</a>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="container">
				<form class="form-signin-ribbon">
					<div class="content-widgets gray">
						<div class=" login-head orange">
							<h3 class="form-signin-heading">Please sign in</h3>
						</div>
						<div class="long-ribbon">
							<div class="ribbon-content">
								<div class="controls input-icon">
									<i class=" icon-user-md"></i>
									<g:ListBox width="301px" height="42px" ui:field="cmd_listbox"
										styleName="{style.span12}" />
									<!-- class="input-block-level" -->

								</div>
								<div class="controls input-icon">
									<i class=" icon-key"></i>
									<g:TextBox ui:field="textBox_2" styleName="{style.textBox}" />
								</div>
								<!-- <label class="checkbox">
									<g:CheckBox />
									Remember me
								</label> -->
								<g:Button ui:field="bnt_login" styleName="{style.button-black}">Sign in</g:Button>
								<!-- class="btn btn-inverse btn-block" -->
							</div>
						</div>
						<div class="forgot-block">
							<h4>Forgot your password ?</h4>
							<p>
                             <g:Anchor ui:field="lien" href="javascript:;"> Click here</g:Anchor>								
								to get your password.
							</p>
									<g:Button ui:field='btn_cancel' styleName="{style.button-green}"> Cancel
							</g:Button>
						</div>
					</div>
				</form>
			</div>
		</div>

	</g:HTMLPanel>
</ui:UiBinder> 