<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>

.page-header {
    font-family: 'Dosis',sans-serif;
    font-size: 16px;
    font-weight: normal;
    line-height: normal;
    margin: 0 0 20px;
}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="906px" height="437px" verticalAlignment="ALIGN_TOP">
			<g:HorizontalPanel width="885px" height="361px" verticalAlignment="ALIGN_BOTTOM" horizontalAlignment="ALIGN_CENTER">
				<g:VerticalPanel width="370px" height="400px" ui:field="vp1" verticalAlignment="ALIGN_BOTTOM">
					<g:Label height="23px" text="General Options" width="266px" addStyleNames="{style.page-header}"/>
					<g:Label text="Name Of Shop" width="266px" height="23px"/>
					<g:TextBox  ui:field="txt_name"/>
					<g:Label height="23px" text="logo shop" width="266px"/>
					<g:FormPanel height="38px" width="356px">
						<g:VerticalPanel width="197px" height="41px" verticalAlignment="ALIGN_MIDDLE">
												<g:HorizontalPanel height="31px" width="337px">
				
				<g:FormPanel  ui:field="pan_form"  height="33px" >
					<g:HorizontalPanel height="25px" width="331px">
						<g:FileUpload width="256px" ui:field="uploadFormElement"/>
						
						<g:Button ui:field="bnt_Upload">
						<i class="icon-upload-alt"></i>
Upload</g:Button>
					</g:HorizontalPanel>
			</g:FormPanel>
			</g:HorizontalPanel>
									
									
									
						</g:VerticalPanel>
					</g:FormPanel>
					<g:Label height="17px" text="Email" width="266px"/>
					<g:TextBox  ui:field="txt_mail"/>
					
					<g:Label height="17px" text="Password Email" width="266px"/>
					<g:TextBox  ui:field="txt_passwordmail"/>
					<g:Label height="23px" text="Management barcodes" width="266px" addStyleNames="{style.page-header}"/>
					<g:HorizontalPanel width="127px" height="33px" verticalAlignment="ALIGN_MIDDLE">
						<g:RadioButton name="new name" />
						<g:Label text="None"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel width="353px" height="33px" verticalAlignment="ALIGN_MIDDLE">
					<g:RadioButton name="new name"/>
					<g:Label text="USB laser shower"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
				<g:VerticalPanel width="355px" height="397px" horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_TOP">
					<g:Label height="21px" text="Tickets For Cash" width="273px" addStyleNames="{style.page-header}"/>
					<g:Label height="23px" text="Header Tickets" width="266px" />
					<g:TextArea width="345px" height="60px" ui:field="txt_entete"/>
					<g:Label height="23px" text="Footers" width="266px"/>
					<g:TextArea height="60px" width="345px" ui:field="txt_pied"/>
					<g:HorizontalPanel width="353px" height="33px" verticalAlignment="ALIGN_MIDDLE">
					<g:CheckBox />
					<g:Label text="Display TVA on the ticket"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel width="353px" height="33px" verticalAlignment="ALIGN_MIDDLE">
					<g:CheckBox />
					<g:Label text="Display the Personal notes on the ticket"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
				
							<g:Cell horizontalAlignment="ALIGN_RIGHT">
								<g:DisclosurePanel open="true" height="116px" width="138px">
				<g:header></g:header>
				<g:VerticalPanel width="132px" height="100px">
					<g:Button width="173px" height="33px" text="Save Data" ui:field="btn_save"/>
					<g:Button height="33px" text="Cancel" width="173px" ui:field="btn_cancel"/>
				</g:VerticalPanel>
			</g:DisclosurePanel>
							</g:Cell>
			
			</g:HorizontalPanel>
		</g:VerticalPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 